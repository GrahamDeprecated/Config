{
    "name": "styleci/config",
    "description": "StyleCI Config Is A Code Style Configuration Manager",
    "keywords": ["Config", "StyleCI", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "symfony/yaml": "^2.7|^3.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^5.0"
    },
    "suggest": {
        "illuminate/support": "Enables Laravel support."
    },
    "autoload": {
        "psr-4": {
            "StyleCI\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StyleCI\\Tests\\Config\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
